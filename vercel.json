{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/static/(.*)",
        "dest": "/public/static/$1"
      },
      {
        "src": "/custom-route",
        "dest": "/about"
      }
    ],
    "redirects": [
      {
        "source": "/old-route",
        "destination": "/new-route",
        "permanent": true
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=0, s-maxage=31536000, stale-while-revalidate"
          },
          {
            "key": "X-Frame-Options",
            "value": "DENY"
          }
        ]
      }
    ],
    "env": {
      "NEXT_PUBLIC_API_URL": "https://art-gallery-blue-theta.vercel.app/"
    }
  }
  